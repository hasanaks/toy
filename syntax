program = stmt* EOF

stmt = printStmt | exprStmt | ";"

printStmt = "print" expr (";" | NEWLINE)
exprStmt = expr (";" | NEWLINE)

expr = additiveExpr
additiveExpr = multiplicativeExpr ("+" | "-") multiplicativeExpr | multiplicativeExpr
multiplicativeExpr = unaryExpr ("*" | "/") unaryExpr | unaryExpr
unaryExpr = ("+" | "-") unaryExpr | atomExpr
atomExpr = NUMBER | grouping

grouping = '(' expr ')'
