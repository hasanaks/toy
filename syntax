program = stmt* EOF

stmt = printStmt | exprStmt | ";"

printStmt = "print" expr ";"
exprStmt = expr ";"

expr = additiveExpr
additiveExpr = multiplicativeExpr ("+" | "-") multiplicativeExpr | multiplicativeExpr
multiplicativeExpr = atomExpr ("*" | "/") atomExpr | atomExpr
atomExpr = NUMBER | grouping

grouping = '(' expr ')'
